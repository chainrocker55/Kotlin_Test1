<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation"
    app:startDestination="@+id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="buu.informatics.s59160545.parking.LoginFragment"
        android:label="Login">
        <action
            android:id="@+id/action_loginFragment_to_parkCarFragment"
            app:destination="@id/parkCarFragment"
            app:popUpTo="@+id/loginFragment" />
        <action
            android:id="@+id/action_loginFragment_to_infoFragment2"
            app:destination="@id/infoFragment"
            app:popUpTo="@+id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/parkCarFragment"
        android:name="buu.informatics.s59160545.parking.ParkCarFragment"
        android:label="Manage Car">
        <action
            android:id="@+id/action_parkCarFragment_to_infoFragment"
            app:destination="@id/infoFragment"
            app:popUpTo="@+id/parkCarFragment" />
    </fragment>
    <fragment
        android:id="@+id/infoFragment"
        android:name="buu.informatics.s59160545.parking.InfoFragment"
        android:label="Profile" />
</navigation>